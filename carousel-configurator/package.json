{
	"name": "carousel-configurator",
	"version": "1.0.0",
	"author": {
    "name": "Adam Argyle",
    "url": "https://nerdy.dev"
  },
	"description": "Build a carousel while learning the CSS to make it.",
  "keywords": [
    "CSS",
    "Scroll Markers",
    "Scroll Buttons",
		"Columns",
    "interactivity: inert"
  ],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "cp package.json build/ && gsutil -m rsync -rd build gs://chrome-dev-demos/public/carousel-configurator"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-netlify": "^4.3.4",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^5.4.14"
	},
	"type": "module",
	"dependencies": {
		"open-props": "^1.7.6"
	}
}
